<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stevensbytes.steven.tstevenhughesclassproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".LinearRelativeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ListViewActivity"></activity>
        <activity android:name=".ViewPagerActivity"></activity>
        <activity android:name=".Hw3Activity"></activity>
        <activity android:name=".lifecycle.ActivityA"
            android:launchMode="standard"></activity>
        <activity android:name=".lifecycle.ActivityB"
            android:launchMode="singleTop"></activity>
        <activity android:name=".lifecycle.ActivityC"
            android:launchMode="singleTask"></activity>
        <activity android:name=".lifecycle.ActivityD"
            android:launchMode="singleInstance"></activity>
    </application>

</manifest>
